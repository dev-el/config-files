[
  {
    "context": "VimControl && !menu",
    "bindings": {
      "space": null,
      "n": ["vim::PushObject", { "around": false }],
      "m": ["vim::PushObject", { "around": true }],
      "ctrl-shift-m": "vim::WindowMiddle",
      "i": "vim::Up",
      "k": "vim::Down",
      "j": "vim::WrappingLeft",
      "l": "vim::WrappingRight",
      "F": "vim::PushReplace",
      "r": ["vim::PushFindForward", { "before": false, "multiline": false }],
      "shift-r": [
        "vim::PushFindBackward",
        { "after": false, "multiline": false }
      ],
      "shift-m": "vim::PushMark",
      "space g r": "editor::OpenExcerpts",
      "space g g": "editor::OpenExcerptsSplit",
      "ctrl-f": "buffer_search::Deploy",
      "ctrl-h": ["buffer_search::Deploy", { "replace_enabled": true }],
      "g space r": ["buffer_search::Deploy", { "replace_enabled": true }],
      "alt-u": "vim::ScrollUp",
      "alt-h": "vim::ScrollDown",
      "space r s j": "vim::ResizePaneLeft",
      "space r s l": "vim::ResizePaneRight",
      "space r s i": "vim::ResizePaneUp",
      "space r s k": "vim::ResizePaneDown",
      "g k h": "vim::SelectNext",
      "g i h": "vim::SelectPrevious",
      "g m": "vim::EndOfLine",
      "g n": "vim::EndOfLineDownward",
      "g h": "vim::StartOfLineDownward",
      "g space h": "vim::StartOfLine",
      "g space j": "editor::ToggleFold",
      ".": "vim::Repeat"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-k": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "space": null,
      "t": null,
      ".": "vim::Repeat",
      "w": "vim::NextWordEnd",
      "e": "vim::NextWordStart",
      "space t j": "workspace::ToggleBottomDock",
      "space h f": "workspace::ToggleRightDock",
      "h": "vim::Redo",
      "H": "vim::Redo",
      "m": "vim::DeleteLeft",
      "shift-m": "vim::PushMark",
      "n": "vim::InsertBefore",
      "f n": "vim::InsertLineBelow",
      "f m": "vim::ChangeToEndOfLine",
      "g c": "editor::ToggleComments",
      "space %": "editor::SelectAll",
      ">": "vim::Indent",
      "<": "vim::Outdent",
      "f e r l": "vim::Indent",
      "f e r j": "vim::Outdent",
      "f e r i": "editor::MoveLineUp",
      "f e r k": "editor::MoveLineDown",
      "space f l": "vim::Indent",
      "space f j": "vim::Outdent",
      "space f i": "editor::MoveLineUp",
      "space f k": "editor::MoveLineDown",
      "ctrl-k": "command_palette::Toggle",
      "space e r": "diagnostics::Deploy",
      "space e f": "editor::Hover",
      "space e c m": "workspace::ToggleZoom",
      "f j": "vim::Search",
      "f i": "vim::MoveToPreviousMatch",
      "f k": "vim::MoveToNextMatch",
      "space r j": "pane::SplitLeft",
      "space r l": "pane::SplitRight",
      "space r i": "pane::SplitUp",
      "space r k": "pane::SplitDown",
      "space r h": "workspace::NewFile",
      "space f f": "pane::CloseActiveItem",
      "space v d": "pane::CloseItemsToTheLeft",
      "space r e": "zed::Quit",
      "space t m": "tab_switcher::Toggle",
      "space t i": "pane::ActivatePreviousItem",
      "space t k": "pane::ActivateNextItem",
      "space t n": "workspace::NewFile",
      "space t h": "pane::CloseActiveItem",
      "space m": "tab_switcher::Toggle",
      "g j": ["workspace::SendKeystrokes", "shift-enter"],
      "shift-n": "vim::InsertAfter",
      "g space shift-n": "vim::InsertLineAbove",
      "o": "editor::Newline",
      "g o": "editor::NewlineBelow",
      "shift-o": "editor::NewlineAbove",
      "space e n": "workspace::NewSearch",
      "space e m": "file_finder::Toggle",
      "space b": "tab_switcher::Toggle",
      "g space b": ["tab_switcher::Toggle", { "select_last": true }],
      "space e j": "workspace::ActivatePaneLeft",
      "space e l": "workspace::ActivatePaneRight",
      "space e i": "workspace::ActivatePaneUp",
      "space e k": "workspace::ActivatePaneDown",
      "space w": "workspace::Save",
      "space n": "project_panel::ToggleFocus",
      "space d": "editor::Hover",
      "f6": "vim::InsertBefore",
      "space c i": "editor::ToggleCodeActions",
      "g r": "editor::FindAllReferences",
      "g space d": "editor::GoToDefinitionSplit",
      "g space t": "editor::GoToTypeDefinitionSplit",
      "g space i": "editor::GoToImplementationSplit",
      "space g i": "pane::GoBack",
      "space g k": "pane::GoForward",
      "space h e f": "editor::ExpandMacroRecursively"
    }
  },
  {
    "context": "vim_mode == helix_normal && !menu",
    "bindings": {
      "escape": "editor::Cancel",
      "ctrl-[": "editor::Cancel",
      "space": null,
      "t": null,
      "w": "vim::NextWordEnd",
      "e": "vim::NextWordStart",
      "c": "editor::SelectLine",
      "shift-c": "vim::Substitute",
      "d": "vim::HelixDelete",
      "delete": "vim::DeleteRight",
      "shift-d": "vim::DeleteToEndOfLine",
      "x": "vim::DeleteRight",
      "shift-x": "vim::DeleteLeft",
      "h": "vim::Redo",
      "H": "vim::Redo",
      "%": "vim::Matching",
      "space %": "editor::SelectAll",
      "space f s": "project_symbols::Toggle",
      "j": "vim::WrappingLeft",
      "l": "vim::WrappingRight",
      "space e c K": "editor::AddSelectionBelow",
      "space e c I": "editor::AddSelectionAbove",
      "space t j": "workspace::ToggleBottomDock",
      "space h f": "workspace::ToggleRightDock",
      "m": "vim::DeleteLeft",
      "n": "vim::InsertBefore",
      "f n": "vim::InsertLineBelow",
      "f m": "vim::ChangeToEndOfLine",
      "g m": "vim::EndOfLine",
      "g n": "vim::EndOfLineDownward",
      "g h": "vim::StartOfLineDownward",
      "g H": "vim::StartOfLine",
      "g c": "editor::ToggleComments",
      "ctrl-k": "command_palette::Toggle",
      "space e r": "diagnostics::Deploy",
      "space e f": "editor::Hover",
      "space e c m": "workspace::ToggleZoom",
      "f j": "vim::Search",
      "f i": "vim::MoveToPreviousMatch",
      "f k": "vim::MoveToNextMatch",
      "space r j": "pane::SplitLeft",
      "space r l": "pane::SplitRight",
      "space r i": "pane::SplitUp",
      "space r k": "pane::SplitDown",
      "space r h": "workspace::NewFile",
      "space f f": "pane::CloseActiveItem",
      "space v d": "pane::CloseItemsToTheLeft",
      "space r e": "zed::Quit",
      "space t m": "tab_switcher::Toggle",
      "space t i": "pane::ActivatePreviousItem",
      "space t k": "pane::ActivateNextItem",
      "space t n": "workspace::NewFile",
      "space t h": "pane::CloseActiveItem",
      "space m": "tab_switcher::Toggle",
      "g j": ["workspace::SendKeystrokes", "shift-enter"],
      "shift-n": "vim::InsertAfter",
      "g space shift-n": "vim::InsertLineAbove",
      "o": "editor::Newline",
      "g o": "editor::NewlineBelow",
      "shift-o": "editor::NewlineAbove",
      "space e n": "workspace::NewSearch",
      "space e m": "file_finder::Toggle",
      "space b": "tab_switcher::Toggle",
      "g space b": ["tab_switcher::Toggle", { "select_last": true }],
      "space f j": "vim::InsertAfter",
      "space e j": "workspace::ActivatePaneLeft",
      "space e l": "workspace::ActivatePaneRight",
      "space e i": "workspace::ActivatePaneUp",
      "space e k": "workspace::ActivatePaneDown",
      "space w": "workspace::Save",
      "space n": "project_panel::ToggleFocus",
      "space d": "editor::Hover",
      "f6": "vim::InsertBefore",
      "space c i": "editor::ToggleCodeActions",
      "g r": "editor::FindAllReferences",
      "g space d": "editor::GoToDefinitionSplit",
      "g space t": "editor::GoToTypeDefinitionSplit",
      "g space i": "editor::GoToImplementationSplit"
    }
  },
  {
    "context": "TabSwitcher",
    "bindings": {
      "i": "menu::SelectPrevious",
      "k": "menu::SelectNext",
      "l": "menu::Confirm",
      "m": "tab_switcher::CloseSelectedItem"
    }
  },
  {
    "context": "vim_mode == visual",
    "bindings": {
      "f6": "vim::SwitchToNormalMode",
      "h": "vim::VisualDelete",
      "n": ["vim::PushObject", { "around": false }],
      "m": ["vim::PushObject", { "around": true }],
      "N": "vim::PushAddSurrounds",
      "w": "vim::NextWordEnd",
      "e": "vim::NextWordStart",
      "f e r l": "vim::Indent",
      "f e r j": "vim::Outdent",
      "f e r i": "editor::MoveLineUp",
      "f e r k": "editor::MoveLineDown",
      "space f l": "vim::Indent",
      "space f j": "vim::Outdent",
      "space f i": "editor::MoveLineUp",
      "space f k": "editor::MoveLineDown"
    }
  },
  {
    "context": "vim_mode == insert",
    "bindings": {
      "f6": "vim::SwitchToNormalMode"
    }
  },
  {
    "context": "vim_mode == replace",
    "bindings": {
      "f6": "vim::SwitchToNormalMode"
    }
  },
  {
    "context": "vim_operator == y",
    "bindings": {
      "h": "vim::PushAddSurrounds"
    }
  },
  {
    "context": "vim_operator == ys",
    "bindings": {
      "h": "vim::CurrentLine"
    }
  },
  {
    "context": "vim_operator == c",
    "bindings": {
      "h": ["vim::PushChangeSurrounds", {}]
    }
  },
  {
    "context": "vim_operator == d",
    "bindings": {
      "h": "vim::PushDeleteSurrounds"
    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "space": null,
      "j": "project_panel::CollapseSelectedEntry",
      "i": "menu::SelectPrevious",
      "k": "menu::SelectNext",
      "space e j": "workspace::ActivatePaneLeft",
      "space e l": "workspace::ActivatePaneRight",
      "space e i": "workspace::ActivatePaneUp",
      "space e k": "workspace::ActivatePaneDown",
      "l": "project_panel::Open",
      "e": "project_panel::OpenPermanent",
      "g w": "pane::CloseActiveItem",
      "space n": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Editor && jupyter && !ContextEditor && vim_mode == normal",
    "bindings": {
      "space r n": "repl::Run",
      "space r m": "repl::RunInPlace",
      "space f e f": "repl::ClearOutputs"
    }
  },
  {
    "context": "Editor && jupyter && !ContextEditor && vim_mode == visual",
    "bindings": {
      "space r n": "repl::Run",
      "space r m": "repl::RunInPlace",
      "space f e f": "repl::ClearOutputs"
    }
  },
  {
    "context": "ContextEditor > Editor && vim_mode == normal",
    "bindings": {
      "space": null,
      "space e j": "workspace::ActivatePaneLeft",
      "space e l": "workspace::ActivatePaneRight",
      "space e i": "workspace::ActivatePaneUp",
      "space e k": "workspace::ActivatePaneDown"
    }
  },
  {
    "context": "Editor && showing_completions",
    "bindings": {
      "enter": "editor::ConfirmCompletion",
      "shift-enter": "editor::ConfirmCompletionReplace",
      "alt-l": "editor::ConfirmCompletion",
      "ctrl-l": "editor::ConfirmCompletion",
      "tab": "editor::ComposeCompletion"
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions)",
    "bindings": {
      "ctrl-n": null,
      "alt-k": "editor::ContextMenuNext",
      "ctrl-k": "editor::ContextMenuNext",
      "alt-i": "editor::ContextMenuPrevious",
      "ctrl-i": "editor::ContextMenuPrevious",

      "up": "editor::MoveLineUp",
      "down": "editor::MoveLineDown",
      "pageup": null,
      "pagedown": null
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions) && vim_mode == normal",
    "bindings": {
      "i": "vim::Up",
      "k": "vim::Down",
      "j": "vim::WrappingLeft",
      "l": "vim::WrappingRight"
    }
  },
  {
    "context": "Picker > Editor",
    "bindings": {
      "alt-j": "menu::Cancel",
      "ctrl-j": "menu::Cancel",
      "alt-i": "menu::SelectPrevious",
      "ctrl-i": "menu::SelectPrevious",
      "alt-k": "menu::SelectNext",
      "ctrl-k": "menu::SelectNext",
      "up": "editor::MoveLineUp",
      "down": "editor::MoveLineDown",
      "alt-l": "picker::ConfirmCompletion",
      "ctrl-l": "picker::ConfirmCompletion"
    }
  }
]
